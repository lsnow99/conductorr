  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Launch Backend",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/conductorr",
      "cwd": "${workspaceFolder}",
      "env": {
        "CONDUCTORR_DEBUG": "true",
        "DB_PATH": "/home/logan/tmp/conductorrtest.sqlite"
      }
    },
    {
      "name": "Test NZBGet",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/internal/integration/nzbget_test.go",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Test Transmission",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/internal/integration/transmission_test.go",
      "cwd": "${workspaceFolder}"
    }
  ]
}
